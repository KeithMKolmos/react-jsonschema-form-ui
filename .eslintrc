{
    "extends": [
      "airbnb"
    ],
    "parser": "babel-eslint",
    "env": {
      "node": true,
      "mocha": true
    },
    "rules": {
      "prefer-destructuring": 0,
      "no-unused-vars": 1,
      "comma-dangle": 0,
      "curly": 0,
      "no-console": 0,
      "no-underscore-dangle": 0,
      "no-plusplus": 0,
      "linebreak-style": 0,
      "indent": ["error", 2],
      "class-methods-use-this": 0,
      "camelcase": 0
    }
  }